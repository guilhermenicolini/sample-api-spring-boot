web: java -Dserver.port=$PORT -Djwt.secret=$JWT_SECRET -Dspring.datasource.url=$SPRING_DATASOURCE_URL -Dspring.datasource.username=$SPRING_DATASOURCE_USERNAME -Dspring.datasource.password=$SPRING_DATASOURCE_PASSWORD -Dspring.datasource.driverClassName=$SPRING_DATASOURCE_DRIVER -Dapi.url=$API_URL $JAVA_OPTS -jar build/libs/*.jar